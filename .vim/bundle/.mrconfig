[DEFAULT]
include = cat /usr/share/mr/git-subtree
git_subtree_test = true
checkout = true
jobs = 1

[characterize]
update = git_subtree_update --squash --prefix=.vim/bundle/characterize https://github.com/tpope/vim-characterize.git master

[grepper]
update = git_subtree_update --squash --prefix=.vim/bundle/grepper https://github.com/mhinz/vim-grepper.git master

[jellybeans]
update = git_subtree_update --squash --prefix=.vim/bundle/jellybeans https://github.com/nanotech/jellybeans.vim.git master

[lspconfig]
update = git_subtree_update --squash --prefix=.vim/bundle/lspconfig https://github.com/neovim/nvim-lspconfig master

[nofrils]
update = git_subtree_update --squash --prefix=.vim/bundle/nofrils https://github.com/robertmeta/nofrils.git master

[repeat]
update = git_subtree_update --squash --prefix=.vim/bundle/repeat https://github.com/tpope/vim-repeat.git master

[scriptease]
update = git_subtree_update --squash --prefix=.vim/bundle/scriptease https://github.com/tpope/vim-scriptease.git master

[securemodelines]
update = git_subtree_update --squash --prefix=.vim/bundle/securemodelines https://github.com/ciaranm/securemodelines.git master

[signify]
update = git_subtree_update --squash --prefix=.vim/bundle/signify https://github.com/mhinz/vim-signify.git master

[SkyBison]
update = git_subtree_update --squash --prefix=.vim/bundle/SkyBison https://github.com/paradigm/SkyBison.git master

[sleuth]
update = git_subtree_update --squash --prefix=.vim/bundle/sleuth https://github.com/tpope/vim-sleuth.git master

[surround]
update = git_subtree_update --squash --prefix=.vim/bundle/surround https://github.com/tpope/vim-surround.git master

[tabular]
update = git_subtree_update --squash --prefix=.vim/bundle/tabular https://github.com/godlygeek/tabular.git master

[tokyonight]
update = git_subtree_update --squash --prefix=.vim/bundle/tokyonight https://github.com/folke/tokyonight.nvim.git main

[unstack]
update = git_subtree_update --squash --prefix=.vim/bundle/unstack https://github.com/mattboehm/vim-unstack.git master
